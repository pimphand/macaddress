# Tambahkan key GPG Centreon
wget -O- https://apt.centreon.com/centreon.gpg | gpg --dearmor -o /usr/share/keyrings/centreon-archive-keyring.gpg

# Tambahkan repository Centreon Bookworm
echo "deb [signed-by=/usr/share/keyrings/centreon-archive-keyring.gpg] https://apt.centreon.com/repository/debian bookworm main" > /etc/apt/sources.list.d/centreon.list


apt install centreon-plugin-Network-Switch-SNMP -y

deb [signed-by=/usr/share/keyrings/centreon-archive-keyring.gpg] https://apt.centreon.com/repository/debian bookworm main

apt install centreon-plugin-applications-protocol-snmp

/usr/lib/centreon/plugins//centreon_generic_snmp.pl \
	--plugin=apps::protocols::snmp::plugin \
	--list-mode

# Lihat daftar interface (pakai community 'public' dan IP switch 192.168.53.10)
snmpwalk -v2c -c public 192.168.53.10 IF-MIB::ifDescr

# Ambil counter spesifik (mis. interface index 5)
snmpget -v2c -c public 192.168.53.10 IF-MIB::ifHCInOctets.5 IF-MIB::ifHCOutOctets.5

snmpget -v2c -c public 192.168.53.10 SNMPv2-MIB::sysName.0

snmpget -v2c -c public 192.168.53.10 1.3.6.1.2.1.1.5.0
